<svg width="800" height="600" xmlns="http://www.w3.org/2000/svg">
  <rect width="100%" height="100%" fill="#111827" />
  
  <!-- Terminal window -->
  <rect x="50" y="50" width="700" height="500" rx="8" fill="#1f2937" stroke="#374151" stroke-width="2" />
  
  <!-- Terminal header -->
  <circle cx="70" cy="70" r="6" fill="#ef4444" />
  <circle cx="90" cy="70" r="6" fill="#f59e0b" />
  <circle cx="110" cy="70" r="6" fill="#10b981" />
  <text x="150" y="75" font-family="monospace" font-size="14" fill="#9ca3af">NeoRealm Terminal v0.1.0</text>
  
  <!-- Terminal content -->
  <text x="70" y="120" font-family="monospace" font-size="14" fill="#10b981">[09:41] SYSTEM: Welcome to NeoRealm</text>
  <text x="70" y="140" font-family="monospace" font-size="14" fill="#10b981">[09:41] SYSTEM: A new signal detected. The Architect acknowledges you, traveler.</text>
  <text x="70" y="160" font-family="monospace" font-size="14" fill="#10b981">[09:41] SYSTEM: Your story begins now.</text>
  <text x="70" y="180" font-family="monospace" font-size="14" fill="#8b5cf6">[09:42] PhantomOS (NetRunner): /me enters the node</text>
  <text x="70" y="200" font-family="monospace" font-size="14" fill="#fbbf24">[09:42] Dice Roll: 1d20 = 15</text>
  
  <!-- Terminal input -->
  <text x="70" y="500" font-family="monospace" font-size="14" fill="#10b981">&gt;</text>
  <text x="85" y="500" font-family="monospace" font-size="14" fill="#ffffff">Type your message or command...</text>
  
  <!-- Cursor -->
  <rect x="350" y="490" width="8" height="16" fill="#10b981" />
</svg>